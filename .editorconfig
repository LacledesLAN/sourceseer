root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_size = 4
indent_style = space
trim_trailing_whitespace = true

[*.go]
indent_size = 8
indent_style = tab

[*.log]
insert_final_newline = false
trim_trailing_whitespace = false

[*.yml]
indent_size = 2
indent_style = space]

[*.win.log]
end_of_line = crlf
